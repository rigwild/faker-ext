{
  "name": "Faker",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Host your social network content externally",
  "options_ui": {
    "page": "public/options.html",
    "open_in_tab": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["linkedin.js"]
    }
  ]
}
